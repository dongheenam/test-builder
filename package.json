    "@types/cookie": "^0.5.1",
    "autoprefixer": "^10.4.13",
    "carbon-icons-svelte": "^11.4.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "iconify-icon": "^1.0.1",
    "postcss": "^8.4.19",
    "postcss-load-config": "^4.0.1",
    "postcss-nested": "^6.0.0",
    "postcss-preset-env": "^7.8.3",
